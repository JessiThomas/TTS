<div class="row">
  <h2> Jessi's Jock Jams</h2>
    <% @songs.each do |song| %>
      <div class="well well-lg">
        <h2><%= song.name %></h2>
        <h3>by <%= song.artist.name %> </h3>
        <p><% "#{song.rating}".to_i.times do |rank| %>
          <span class="glyphicon glyphicon-headphones"></span>
        <% end %>
        </p>
      </div>
    <% end %>
</div>
